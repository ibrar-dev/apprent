<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <%= use_log_rocket(@conn) %>
    <title>AppRent</title>
		<script>
			window.user_token = '<%= @conn.assigns.user_token %>';
			window.current_user_id = '<%= @conn.assigns.user.id %>';
		</script>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="icon" href="<%= Routes.static_path @conn, "/images/icon.png" %>" sizes="any" type="image/png">
    <link rel="stylesheet" href="<%= Routes.static_path(@conn, "/css/users.css") %>">
    <!-- Start of apprent Zendesk Widget script -->
    <script id="ze-snippet" src="https://static.zdassets.com/ekr/snippet.js?key=b99f7242-1caa-46a1-afff-0e689c2e9a68"> </script>
    <!-- End of apprent Zendesk Widget script -->
    <script src="<%= Routes.static_path(@conn, "/js/users.js") %>"></script>
  </head>
  <body style="background-color: white;">
    <div class="container-fluid">
      <div class="row">
        <main class="main-content w-100">
          <%= render "_header.html", conn: @conn %>
          <%= render "_flash.html", conn: @conn %>
          <div class="main-content-container container-fluid px-4">
            <%= @inner_content %>
          </div>
          <%= render "_footer.html", conn: @conn %>
        </main>
      </div>
    </div>
    <%= render "_modal.html", conn: @conn%>
    <script>
      $('.dropdown-toggle').dropdown()
    </script>
  </body>
</html>
